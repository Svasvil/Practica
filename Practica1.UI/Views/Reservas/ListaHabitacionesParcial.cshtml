        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaInicioReserva)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaFinReserva)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaDeRegistro)
        </td>
